{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/db-status",
      "dest": "/api/db-status.js"
    },
    {
      "src": "/api/init-db",
      "dest": "/api/init-db.js"
    },
    {
      "src": "/api/hello",
      "dest": "/api/hello.js"
    },
    {
      "src": "/api",
      "dest": "/api/_app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}